@article{dayNumericalSimulationLaminar2000,
  title = {Numerical Simulation of Laminar Reacting Flows with Complex Chemistry},
  author = {Day, M S and Bell, J B},
  date = {2000-12},
  journaltitle = {Combustion Theory and Modelling},
  shortjournal = {Combustion Theory and Modelling},
  volume = {4},
  number = {4},
  pages = {535--556},
  issn = {1364-7830, 1741-3559},
  doi = {10.1088/1364-7830/4/4/309},
  url = {http://www.tandfonline.com/doi/abs/10.1088/1364-7830/4/4/309},
  urldate = {2020-06-29},
  abstract = {We present an adaptive algorithm for low Mach number reacting flows with complex chemistry. Our approach uses a form of the low Mach number equations that discretely conserves both mass and energy. The discretization methodology is based on a robust projection formulation that accommodates large density contrasts. The algorithm uses an operator-split treatment of stiff reaction terms and includes effects of differential diffusion. The basic computational approach is embedded in an adaptive projection framework that uses structured hierarchical grids with subcycling in time that preserves the discrete conservation properties of the underlying single-grid algorithm. We present numerical examples illustrating the performance of the method on both premixed and nonpremixed flames.},
  langid = {english}
}

@inproceedings{debonisSolutionsTaylorGreenVortex2013,
  title = {Solutions of the {{Taylor-Green Vortex Problem Using High-Resolution Explicit Finite Difference Methods}}},
