  keywords = {Animals,Aqueous Humor,Blood Pressure,Clonidine,Glaucoma,Hemodynamics,Humans,Hydrogen-Ion Concentration,Intraocular Pressure,Ophthalmic Artery,Ophthalmic Solutions,Rabbits}
