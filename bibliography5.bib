}

@article{kummerExtensionDiscontinuousGalerkin2013,
  title = {An {{Extension}} of the {{Discontinuous Galerkin Method}} for the {{Singular Poisson Equation}}},
  author = {Kummer, Florian and Oberlack, Martin},
