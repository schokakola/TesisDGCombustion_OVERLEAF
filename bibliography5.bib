@article{abbassiEntropyGenerationPoiseuille2003,
  title = {Entropy Generation in {{Poiseuille}}\textendash{{Benard}} Channel Flow},
  author = {Abbassi, Hassen and Magherbi, Mourad and Ben Brahim, Ammar},
  year = {2003},
  month = dec,
  journal = {International Journal of Thermal Sciences},
  volume = {42},
  number = {12},
  pages = {1081--1088},
  issn = {12900729},
  doi = {10.1016/S1290-0729(03)00095-4},
  abstract = {The issue of entropy generation in Poiseuille\textendash Benard channel flow is analyzed by solving numerically the mass, momentum and energy equations with the use of the classic Boussinesq incompressible approximation. The numerical scheme is based on Control Volume Finite Element Method with the SIMPLER algorithm for pressure\textendash velocity coupling. Results are obtained for Rayleigh numbers Ra and irreversibility {$\varphi$} ranging from 103 to 5 \texttimes{} 104 and from 10-4 to 10 respectively. Variations of entropy generation and the Bejan number as a function of Ra and {$\varphi$} are studied. The limit value {$\varphi$}l for which entropy generation due to heat transfer is equal to entropy due to fluid friction is evaluated. It has been found that {$\varphi$}l is a decreasing function of the Rayleigh number Ra. {$\varphi$}l varies from 0.0015 to 0.096 when Ra decrease from 5 \texttimes{} 104 to 103. Stream lines and entropy generation maps are plotted at six times over one period at Ra =104 and {$\varphi$} = 10-3. It has been found that the maximum entropy generation is localized at areas where heat exchanged between the walls and the flow is maximum. No significant entropy production is seen in the main flow.},
  langid = {english},
  file = {C\:\\Users\\jfgj8\\Zotero\\storage\\B2CA55ZS\\Abbassi et al. - 2003 - Entropy generation in Poiseuille–Benard channel fl.pdf}
}

